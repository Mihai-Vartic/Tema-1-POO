#include <iostream>

using namespace std;

class Vector
{
private:
    int n, *v;
   
public:
    Vector(int n, int x)
    {
        this -> n = n;
        v = new int[n];
        for(int i = 0; i < n; i++)
            v[i] = x;
    }
   
    Vector(Vector &ob){};   //Copiere
   
    int getN()
    {
        return n;
    }
   
    int operator[](int i)
    {
        return v[i];
    }
   
    ~Vector()
    {
        delete[] v;
    }
};

int main()
{
    Vector v(3, 5);
    for(int i = 0; i < v.getN(); i++)
       cout<<v[i]<<' ';
    return 0;
}
